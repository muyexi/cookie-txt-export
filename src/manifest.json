{
  "name": "cookie.txt export",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Exports cookie data in a format that can be used by wget",
  "permissions": [ "cookies", "tabs", "http://*/*", "https://*/*" ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }  
}
